<?xml version="1.0" encoding="utf-8"?>
<!--
    ic_volume_up.xml - Vector icon for volume up button

    This is a vector drawable (like SVG) that draws the volume up icon.
    Vector drawables are resolution-independent and look sharp on all screens.

    For beginners: This defines the shape and appearance of the volume up icon
    using paths (similar to SVG graphics).
-->
<vector xmlns:android="http://schemas.android.com/apk/res/android"
    android:width="24dp"
    android:height="24dp"
    android:viewportWidth="24"
    android:viewportHeight="24">

    <!-- Speaker shape -->
    <path
        android:fillColor="#FFFFFF"
        android:pathData="M3,9v6h4l5,5V4L7,9H3z" />

    <!-- Volume waves (3 curves) -->
    <path
        android:fillColor="#FFFFFF"
        android:pathData="M16.5,12c0,-1.77 -1.02,-3.29 -2.5,-4.03v8.05c1.48,-0.73 2.5,-2.25 2.5,-4.02z" />

    <path
        android:fillColor="#FFFFFF"
        android:pathData="M14,3.23v2.06c2.89,0.86 5,3.54 5,6.71s-2.11,5.85 -5,6.71v2.06c4.01,-0.91 7,-4.49 7,-8.77s-2.99,-7.86 -7,-8.77z" />
</vector>